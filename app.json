{
  "expo": {
    "name": "Kasengo",
    "slug": "Kasengo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Casengo.png",
    "scheme": "casengo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Casengo.png"
        // "backgroundColor": "#ed7d2d"
      },
      "package": "com.russelharv.casengo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Casengo.png",
          "resizeMode": "cover",
          // "backgroundColor": "#ed7d2d",
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f544f7f-558d-4639-8564-980e5616f8f3"
      }
    }
  }
}
